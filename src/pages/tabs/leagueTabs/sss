  <div>
          {data?.data.length ? 
            groupedSchedule.map((dailyMatch, key) => {

                 return   
                    <div key={key}> 
                      <div className="pl-base pt-md text-sm h-11 bg-primary-100">          {dailyMatch.key}       </div>    {dailyMatch.values.map((match, key)=>{return <div key={key}className="flex justify-between items-center h-20 py-mdd px-base">
            <div>
           
              <div className="text-sm">
                {match.startHour}
              </div>
              <div className="text-xs text-center text-grayscale-300 bg-grayscale-100 rounded-lg mt-2">
                {status[match.status]}
              </div>

            </div>

            <div></div>
          </div>
        }
          )}    
              
          ) : (
              <div>일정이 없습니다.</div>
          ) }


        </div>